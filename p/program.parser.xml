<parserFile>
 <grammar name="ProgramParser">
  <rule first="{PROGRAM}" name="Program">
   <group first="{PROGRAM}">
    <sequence first="{PROGRAM}">
     <sequence first="{PROGRAM}">
      <sequence first="{PROGRAM}">
       <sequence first="{PROGRAM}">
        <sequence first="{PROGRAM}">
         <nonterminal first="{PROGRAM}" ruleName="ProgramHeading"/>
         <token first="{SEMICOLON}" name="SEMICOLON"/>
        </sequence>
        <optional first="{#, USES}">
         <nonterminal first="{USES}" ruleName="UsesClause"/>
        </optional>
       </sequence>
       <action first="{#}">
        <empty first="{#}"/>
       </action>
      </sequence>
      <nonterminal first="{#, BEGIN, CONST, CONSTRUCTOR, FUNCTION, PROCEDURE, TYPE, VAR}" ruleName="Block"/>
     </sequence>
     <action first="{DOT}">
      <token first="{DOT}" name="DOT"/>
     </action>
    </sequence>
   </group>
  </rule>
  <rule first="{PROGRAM}" name="ProgramHeading">
   <action first="{PROGRAM}">
    <group first="{PROGRAM}">
     <sequence first="{PROGRAM}">
      <sequence first="{PROGRAM}">
       <token first="{PROGRAM}" name="PROGRAM"/>
       <nonterminal first="{ID}" ruleName="ProgramId"/>
      </sequence>
      <optional first="{#, LPAREN}">
       <nonterminal first="{LPAREN}" ruleName="ProgramParameterList"/>
      </optional>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{ID}" name="ProgramId">
   <action first="{ID}">
    <group first="{ID}">
     <sequence first="{ID}">
      <action first="{ID}">
       <token first="{ID}" name="ID"/>
      </action>
      <kleene first="{#, DOT}">
       <group first="{DOT}">
        <sequence first="{DOT}">
         <token first="{DOT}" name="DOT"/>
         <action first="{ID}">
          <token first="{ID}" name="ID"/>
         </action>
        </sequence>
       </group>
      </kleene>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{LPAREN}" name="ProgramParameterList">
   <sequence first="{LPAREN}">
    <sequence first="{LPAREN}">
     <token first="{LPAREN}" name="LPAREN"/>
     <optional first="{#, ID}">
      <nonterminal first="{ID}" ruleName="IdentifierList"/>
     </optional>
    </sequence>
    <token first="{RPAREN}" name="RPAREN"/>
   </sequence>
  </rule>
  <rule first="{USES}" name="UsesClause">
   <sequence first="{USES}">
    <sequence first="{USES}">
     <sequence first="{USES}">
      <token first="{USES}" name="USES"/>
      <action first="{ID}">
       <nonterminal first="{ID}" ruleName="UnitName"/>
      </action>
     </sequence>
     <kleene first="{#, COMMA}">
      <group first="{COMMA}">
       <sequence first="{COMMA}">
        <token first="{COMMA}" name="COMMA"/>
        <action first="{ID}">
         <nonterminal first="{ID}" ruleName="UnitName"/>
        </action>
       </sequence>
      </group>
     </kleene>
    </sequence>
    <token first="{SEMICOLON}" name="SEMICOLON"/>
   </sequence>
  </rule>
  <rule first="{ID}" name="UnitName">
   <action first="{ID}">
    <group first="{ID}">
     <sequence first="{ID}">
      <action first="{ID}">
       <token first="{ID}" name="ID"/>
      </action>
      <kleene first="{#, DOT}">
       <group first="{DOT}">
        <sequence first="{DOT}">
         <token first="{DOT}" name="DOT"/>
         <action first="{ID}">
          <token first="{ID}" name="ID"/>
         </action>
        </sequence>
       </group>
      </kleene>
     </sequence>
    </group>
   </action>
  </rule>
 </grammar>
</parserFile>
